    @Html.HiddenFor(m => m._TrustCompanyID);
    @Html.HiddenFor(m => m._TrustCompanyAddressID);
    @Html.HiddenFor(m => m.Shareholders[0]._ShareholderID);
    @Html.HiddenFor(m => m.Shareholders[1]._ShareholderID);                                                                       
    @Html.HiddenFor(m => m.Shareholders[2]._ShareholderID);                                                                       
         
    @Html.HiddenFor(m => m._NameTrust, new { id = "NameTrustTC" })
