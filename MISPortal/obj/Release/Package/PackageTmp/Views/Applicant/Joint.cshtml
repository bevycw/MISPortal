                     <p101>
                                Are you a US citizen, resident, trust, or entity registered in the USA?
                    </p101>
                    </div>
                     <div class="us_ctzn_nac mar-lt-110">
                                @Html.RadioButtonFor(m => m[0]._IsUSCitizen, true, new { onclick = "ShowHideElement('div_ap1FATCA',true)" })Yes
                                @Html.RadioButtonFor(m => m[0]._IsUSCitizen, false, new { @checked = "checked", onclick = "ShowHideElement('div_ap1FATCA',false)" })No
                    </div>
                    <div id="div_ap1FATCA" class="mar-lt-110" style="display:none;">
                        <h3>FATCA</h3>
                        <p>This section only applies to United States Citizens/entities.<br />
                            You will <ins><b>not</b></ins> be FATCA exempt if you are a US person entity linked to a US financial institution or if you are an entity where a US person has a controlling interest (greater than 25%).<br />
                            <b>Are you FATCA exempt?</b><br />
                            @Html.CheckBoxFor(m => m[0]._IsFATCA, new { @class = "chkap1FATCA",value=true })
                            Yes
                            @Html.CheckBoxFor(m => m[0]._IsFATCA,  new { @class="chkap1FATCA",value=false })
                            No - if you tick this box, you need to complete the FATCA form in addition to this form. Please contact Investor Services.
                        </p>
                    </div>
                    <div class="but_one mar-lt-110">
                    <span class="radio_buttonnext"><a href="#div_j-app-2" rel="" id="t-detail" class="btn btn-primary btn-oval btn-large">
                        Next</a></span></div>
                </div><!--/control-group-->
                </div>
                </div>
                            
                </fieldset>      
                </div><!--/frm_tg_cd-->
            </div>
                      </div><!--/control-group-->
                    </div>
                 </div><!--/row-->
                 </fieldset>      
             </div><!--/frm_tg_cd-->
                </div>
            </div><!--/row-->
