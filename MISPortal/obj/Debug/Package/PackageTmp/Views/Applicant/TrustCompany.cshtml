                                 @Html.TextBoxFor(m => m._ACN_ABN_Company, new { @class = "fnl_txtbx_lrg", placeholder = "ACN / ABN", @required = "required" })<br />
                                 @Html.TextBoxFor(m => m._AusTFN_Company, new { @class = "fnl_txtbx_lrg", placeholder = "Australian Tax File Number (TFN)", @required = "required" })<br />
